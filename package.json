{
  "name": "playwright-typescript-cucumber-framework",
  "version": "1.0.0",
  "description": "Comprehensive Playwright TypeScript Cucumber test automation framework",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js --require-module ts-node/register --require src/steps/**/*.ts --format json:reports/cucumber-report.json --format html:reports/cucumber-report.html --publish-quiet",
    "test:headless": "npm run test -- --headless",
    "test:headed": "npm run test -- --headed",
    "test:parallel": "cucumber-js --require-module ts-node/register --require src/steps/**/*.ts --parallel 4",
    "test:chrome": "cross-env BROWSER=chrome npm run test",
    "test:firefox": "cross-env BROWSER=firefox npm run test",
    "test:safari": "cross-env BROWSER=safari npm run test",
    "test:edge": "cross-env BROWSER=edge npm run test",
    "test:tag": "cucumber-js --require-module ts-node/register --require src/steps/**/*.ts --tags",
    "report:generate": "node src/utils/enhanced-reporter.js",
    "report:clean": "node src/utils/enhanced-reporter.js --clean",
    "report:open": "npm run report:generate && start reports/index.html",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "report:allure:generate": "allure generate reports --clean --output reports/allure-report",
    "report:allure:open": "allure open reports/allure-report",
    "test:with:report": "npm run test && npm run report:generate",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf reports/* allure-results allure-report test-results",
    "install:browsers": "npx playwright install",
    "health-check": "node -e \"console.log('Health check passed'); process.exit(0)\"",
    "docker:build": "docker build -f docker/Dockerfile -t playwright-cucumber .",
    "docker:build:dev": "docker build -f docker/Dockerfile --target development -t playwright-cucumber:dev .",
    "docker:build:ci": "docker build -f docker/Dockerfile --target ci-optimized -t playwright-cucumber:ci .",
    "docker:build:prod": "docker build -f docker/Dockerfile --target production -t playwright-cucumber:prod .",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:up:dev": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml --profile dev up -d",
    "docker:up:ci": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.ci.yml --profile ci up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:test": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml run --rm playwright-tests",
    "docker:test:chrome": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml run --rm -e BROWSER=chrome playwright-tests",
    "docker:test:firefox": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml run --rm -e BROWSER=firefox playwright-tests",
    "docker:test:edge": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml run --rm -e BROWSER=edge playwright-tests",
    "docker:test:smoke": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml run --rm -e TAGS=@smoke playwright-tests",
    "docker:test:regression": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml run --rm -e TAGS=@regression playwright-tests",
    "docker:test:parallel": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.ci.yml run --rm -e PARALLEL_WORKERS=4 playwright-tests",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "docker:shell": "docker-compose -f docker/docker-compose.yml exec playwright-tests /bin/bash",
    "docker:clean": "docker-compose -f docker/docker-compose.yml down -v --remove-orphans && docker system prune -f",
    "docker:reports": "docker-compose -f docker/docker-compose.yml --profile reporting up -d nginx-reports",
    "docker:status": "docker-compose -f docker/docker-compose.yml ps",
    "docker:selenium": "docker-compose -f docker/docker-compose.yml up -d selenium-hub chrome-node firefox-node edge-node",
    "docker:monitoring": "docker-compose -f docker/docker-compose.yml --profile monitoring up -d prometheus grafana",
    "jira:example": "node examples/jira-example.js",
    "jira:test-connection": "node -e \"const {JiraIntegration} = require('./src/utils/integrations/jira-integration'); new JiraIntegration().testConnection().then(r => console.log(r ? 'Connected' : 'Failed'))\"",
    "testrail:example": "node examples/testrail-example.js",
    "testrail:test-connection": "node -e \"const {TestRailIntegration} = require('./src/utils/integrations/testrail-integration'); new TestRailIntegration().testConnection().then(r => console.log(r ? 'Connected' : 'Failed'))\"",
    "test-mgmt:example": "node examples/test-management-example.js",
    "test-mgmt:sync": "node -e \"const {TestManagementIntegration} = require('./src/utils/integrations/test-management-integration'); new TestManagementIntegration().testConnections().then(console.log)\"",
    "test-mgmt:update-from-cucumber": "node -e \"const {TestManagementIntegration} = require('./src/utils/integrations/test-management-integration'); const tm = new TestManagementIntegration(); tm.updateResultsFromCucumberReport('./reports/cucumber-report.json').then(() => console.log('Updated'))\"",
    "db:migrate": "node src/utils/database/migrate.js migrate",
    "db:rollback": "node src/utils/database/migrate.js rollback",
    "db:test": "node src/utils/database/migrate.js test",
    "db:demo": "node examples/database-demo.js",
    "db:example": "node examples/database-example.js",
    "test:database": "cucumber-js --require-module ts-node/register --require src/steps/**/*.ts --tags '@database'",
    "test:database:mysql": "cucumber-js --require-module ts-node/register --require src/steps/**/*.ts --tags '@database and @mysql'",
    "test:database:mongodb": "cucumber-js --require-module ts-node/register --require src/steps/**/*.ts --tags '@database and @mongodb'",
    "test:database:postgresql": "cucumber-js --require-module ts-node/register --require src/steps/**/*.ts --tags '@database and @postgresql'",
    "test:database:crud": "cucumber-js --require-module ts-node/register --require src/steps/**/*.ts --tags '@database and @crud'",
    "test:database:performance": "cucumber-js --require-module ts-node/register --require src/steps/**/*.ts --tags '@database and @performance'",
    "encrypt:data": "node src/utils/encryption/encrypt-data.js",
    "decrypt:data": "node src/utils/encryption/decrypt-data.js"
  },
  "keywords": [
    "playwright",
    "typescript",
    "cucumber",
    "bdd",
    "automation",
    "testing",
    "framework"
  ],
  "author": "Test Automation Team",
  "license": "MIT",
  "devDependencies": {
    "@cucumber/cucumber": "^10.0.1",
    "@cucumber/html-formatter": "^21.0.0",
    "@cucumber/messages": "^30.1.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@playwright/test": "^1.40.0",
    "@types/chai": "^4.3.6",
    "@types/crypto-js": "^4.1.3",
    "@types/csvtojson": "^2.0.0",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "allure-commandline": "^2.34.1",
    "cross-env": "^7.0.3",
    "cucumber-html-reporter": "^7.2.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-playwright": "^0.16.0",
    "multiple-cucumber-html-reporter": "^3.9.3",
    "open-cli": "^7.2.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "allure-playwright": "^2.8.1",
    "archiver": "^6.0.1",
    "axios": "^1.5.1",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.12",
    "crypto-js": "^4.2.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "faker": "^6.6.6",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "jimp": "^0.22.10",
    "moment": "^2.29.4",
    "mongodb": "^6.2.0",
    "mysql2": "^3.6.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "playwright": "^1.40.0",
    "unzipper": "^0.10.14",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/playwright-typescript-cucumber-framework.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/playwright-typescript-cucumber-framework/issues"
  },
  "homepage": "https://github.com/your-username/playwright-typescript-cucumber-framework#readme"
}
